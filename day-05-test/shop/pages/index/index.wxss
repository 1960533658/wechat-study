/**index.wxss**/
.fl {
	float: left;
}
.fr {
	float: right;
}
.clearfix:before, .clearfix:after {
	content: "";
	display: table;
}
.main {
  width: 100%;
}
.navigation {
  width: 100%;
  height: 170rpx;
  padding-top: 110rpx;
  background-image: url(data:image/jpg;base64,);
  background-size: cover;
  text-align: center;
  font-size: 32rpx;
  color: #ffffff;
  box-sizing: border-box;
}
/* 内容部分开始 */
.main {
  padding: 20rpx 20rpx;
  background-color: #f7f7f8;
  box-sizing: border-box;
}
swiper {
  margin-bottom: 26rpx;
}
swiper image {
  width: 100%;
}
/* 分类文字 */
.fication {
  display: flex;
  justify-content: space-around;
  border-bottom: 5rpx solid transparent;
}
.fication text {
  padding-bottom: 12rpx;
  margin-bottom: 8rpx;
  font-size: 28rpx;
}
.fication text:first-child {
  border-bottom: 5rpx solid #27b89a;
}
/* 商品 */
.goods {
  
  width: 100%;
}
.goods .left, .goods .right {
  width: 350rpx;
  background-color: #fff;
}
.goods image {
  width: 100%;
  height: 330rpx;
  margin-bottom: 20rpx;
}
.goods .left:first-child image:first-child {
  height: 200rpx;
}
.text {
  position: relative;
  padding-bottom: 60rpx;
}
.text text {
  display: block;
}
.text .make {
  font-size: 26rpx;
  color: #000;
}
.text .monry {
  display: inline;
  font-size: 24rpx;
  color: #cf4444;
}
.text .up {
  display: inline;
  font-size: 24rpx;
  color: #ccc;
}
.about image {
  width: 20rpx;
  height: 20rpx;
}
.about text {
  font-size: 18rpx;
  padding-left: 5rpx;
}
.about text:nth-child(2) {
  padding-right: 20rpx;
}
.about image:last-child {
  position: absolute;
  right: 12rpx;
  bottom: 12rpx;
  width: 82rpx;
  height: 82rpx;
}
.left, .right {
  display: inline-block;
  margin-bottom: 17rpx;
}
.about image:first-child {
  height: 20rpx;
}
.box1,.box2 {
  float: left;
  width: 350rpx;
}
.box1 {
  margin-right: 10rpx;
}



